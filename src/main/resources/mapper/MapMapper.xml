<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.smarthospital_back.mapper.MapMapper">

    <!-- 生成缴费单 -->
    <insert id="insertMap" parameterType="org.example.smarthospital_back.entity.MapEntity">
        insert into Map (m_name, m_pic_url) values
            (#{m_name}, #{m_pic_url});
    </insert>

    <!-- 检查缴费单 -->
    <select id="getMap" resultType="org.example.smarthospital_back.entity.MapEntity">
        select m_map_url from Map where m_id = #{m_id};
    </select>
</mapper>